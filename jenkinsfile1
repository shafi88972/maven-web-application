node{
def mavenHome = tool name: "maven3.9.1"

   stage ('checkoutcode'){
     git 'https://github.com/nani0231/maven-web-application.git' 
       
   }
     
   stage ('build'){
     sh "${mavenHome}/bin/mvn clean package"
       
   }
   stage ('Artifacts'){
     sh "${mavenHome}/bin/mvn deploy"  
   }
}
